{
  "swagger": "2.0",
  "info": {
    "title": "DiscordStorage API",
    "description": "API endpoints for using DiscordStorage",
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/folder/{folderName}": {
      "post": {
        "description": "",
        "parameters": [
          {
            "name": "folderName",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/folder/{folderID}/files": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name": "folderID",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/folders": {
      "get": {
        "description": "",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/download/{folderID}/{fileID}": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name": "folderID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "fileID",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/upload/{folderID}": {
      "post": {
        "description": "",
        "consumes": [
          "multipart/form-data"
        ],
        "parameters": [
          {
            "name": "folderID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "singleFile",
            "in": "formData",
            "type": "file",
            "required": true,
            "description": "The file to upload"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    }
  }
}